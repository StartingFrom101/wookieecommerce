 function(props) {\r\n    const navigation = useNavigate();\r\n    return <LoginForm {...props} navigate={navigation} />;\r\n};\r\n","D:\\BachkhoaAptech\\Classes\\Term2\\ReactJs\\project01\\src\\validation.js",[],[],"D:\\BachkhoaAptech\\Classes\\Term2\\ReactJs\\project01\\src\\components\\pages\\searchres\\index.js",[],[],"D:\\BachkhoaAptech\\Classes\\Term2\\ReactJs\\project01\\src\\components\\pages\\cart\\index.js",["112"],[],{"ruleId":"113","severity":1,"message":"114","line":13,"column":10,"nodeType":"115","messageId":"116","endLine":13,"endColumn":19},{"ruleId":"113","severity":1,"message":"117","line":13,"column":21,"nodeType":"115","messageId":"116","endLine":13,"endColumn":29},{"ruleId":"118","severity":1,"message":"119","line":33,"column":6,"nodeType":"120","endLine":33,"endColumn":8,"suggestions":"121"},{"ruleId":"118","severity":1,"message":"122","line":20,"column":8,"nodeType":"120","endLine":20,"endColumn":10,"suggestions":"123"},{"ruleId":"113","severity":1,"message":"124","line":1,"column":10,"nodeType":"115","messageId":"116","endLine":1,"endColumn":17},{"ruleId":"118","severity":1,"message":"125","line":28,"column":6,"nodeType":"120","endLine":28,"endColumn":8,"suggestions":"126"},{"ruleId":"113","severity":1,"message":"127","line":120,"column":13,"nodeType":"115","messageId":"116","endLine":120,"endColumn":23},{"ruleId":"113","severity":1,"message":"128","line":122,"column":30,"nodeType":"115","messageId":"116","endLine":122,"endColumn":48},{"ruleId":"129","severity":1,"message":"130","line":195,"column":1,"nodeType":"131","endLine":198,"endColumn":2},{"ruleId":"113","severity":1,"message":"132","line":2,"column":8,"nodeType":"115","messageId":"116","endLine":2,"endColumn":11},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'page.page'. Either include it or remove the dependency array.","ArrayExpre